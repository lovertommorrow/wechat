.homeContainer{
  width: 100%;
  height: 100%;
  overflow-x: hidden;
}
.containTop{
  width: 100%;
  height: 470rpx;
  position: relative;
}
.containTop image{
  height: 470rpx;
}
.welcome{
  width:100%;
  text-align: center;
  position: absolute;
  color: #fff;
  text-shadow: 0px 2px 2px rgba(0, 0, 0, 0.25);
}
.msg{
  top: 199rpx;
  font-size: 36rpx;
}
.date{
  top: 265rpx;
  font-size: 28rpx;
}
.containBottom{
  width: 100%;
  margin: 60rpx;
  display:flex;
  flex-wrap: wrap;
  /* margin-top: 60rpx;
  margin-left: 60rpx;
  margin-bottom: 60rpx; */
}
.btnItem{
  width: 136rpx;
  position: relative;
}
.btnItem image{
  height: 136rpx;
}
.btnItemTitle{
  margin-top: 30rpx;
  text-align: center; 
  font-size: 30rpx;
  font-weight: 400;
  color: #333333;
}
.num{
  position: absolute;
  top:0;
  left:102rpx;
  width: 60rpx;
  height: 50rpx;
  background: #FF3A31;
  border-radius: 25rpx;
  text-align: center;
  line-height: 50rpx;
  font-size: 32rpx;
  font-weight: 400;
  color: #FFFFFF;
}