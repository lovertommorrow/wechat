.tabNav{
  z-index: 4;
  width:100%; 
  height:80rpx; 
  line-height: 80rpx; 
  background: #fff; 
  display: flex; 
  padding:0 30rpx; 
  border-bottom:1px solid #f5f5f5;
  box-sizing: border-box; 
}
.tabNav> view{
  text-align: center;
  margin-right:50rpx;
}
.tabNav> view:last-child{ 
  margin-right: 0;
}
.tabNav> view text{
  padding:0 15rpx;
  height:75rpx;
  display:inline-block;
}
.tabNav .cur text{ 
  border-bottom:5rpx solid #ffcd32;
  color:#ffcd32;
}